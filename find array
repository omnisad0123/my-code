#include <bits/stdc++.h>
#define ll long long int
#define endl "\n"
using namespace std;

void solve()
{
    ll n;
    cin>>n;
    if(n==1)
        cout<<1<<endl;
    else
        {for(ll i=2;i<n+2;i++)
            cout<<i<<" ";
         cout<<endl;
        }
    


}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    ll t;
    cin>>t;


    while(t--)
    { 

        solve();

    }
return 0;
}
