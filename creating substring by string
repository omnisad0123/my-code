using namespace std;
void substring( char str[], int n){
    for(int len=0;len<=n;len++){
        for(int i=0;i<=n-len;i++){
            int j=i+len-1;
            for(int k=i;k<=j;k++){
                cout<<str[k];
               
            } cout<<endl;
        }
    }
}
